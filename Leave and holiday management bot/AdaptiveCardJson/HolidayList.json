{
    "type": "AdaptiveCard",
    "body": [
        {
            "type": "Image",
            "horizontalAlignment": "Center",
            "url": "data:image/png;base64,",
            "size": "Large",
            "width": "100px"
        },
        {
            "type": "TextBlock",
            "id": "holiday_date",
            "horizontalAlignment": "Center",
            "spacing": "None",
            "size": "Large",
            "weight": "Bolder",
            "color": "Dark",
            "text": "{holiday_date}"
        },
        {
            "type": "ColumnSet",
            "separator": true,
            "columns": [
                {
                    "type": "Column",
                  "items": [
                    {
                      "type": "TextBlock",
                      "id": "holiday_description",
                      "horizontalAlignment": "Center",
                      "spacing": "None",
                      "size": "Large",
                      "color": "Accent",
                      "text": "{holiday_description}"
                    }
                  ],
                    "width": 1
                }
            ]
        }
    ],
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.0",
    "speak": "<s>List of holidays</s>"
}